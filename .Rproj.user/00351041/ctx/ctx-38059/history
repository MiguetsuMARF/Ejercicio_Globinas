ggmsa("Raw_Data/DivergentGlobins.fasta")
library (ggmsa)
ggmsa("Raw_Data/DivergentGlobins.fasta")
library (BioStrings)
if (!require("BiocManager", quietly = TRUE))
install.packages("BiocManager")
BiocManager::install(version = "3.19")
library (ggmsa)
library (BioStrings)
globinas <- readDNAStringSet("Raw_Data/DivergentGlobins.fasta")
library (BioStrings)
library (Biostrings)
globinas <- readDNAStringSet("Raw_Data/DivergentGlobins.fasta")
pdf ("Resultados/Alineamiento_globinas.pdf")
ggmsa(globinas)
dev.off()
Alineamiento_1 <- pairwiseAlignment (globinas, type = "global")
msa (globinas)
library (msa)
msa (globinas)
alineamiento_1 <- msa (globinas)
Matriz_Distancias <- seqinr::dist.alignment (msaConvert (Alineamiento_1,"seqinr::alignment"))
Matriz_Distancias <- seqinr::dist.alignment (msaConvert (alineamiento_1,"seqinr::alignment"))
Matriz_Distancias
Arbol_nj <- ape::nj (Matriz_Distancias)
plot (Arbol_nj)
plot (Arbol_nj, "unrooted")
Arbol_upgma <- phangorn::upgma (Matriz_Distancias)
plot (Arbol_upgma)
plot (Arbol_nj, "unrooted")
plot (Arbol_nj)
plot (Arbol_upgma)
pdf ("Resultados/Arboles_globinas.pdf")
plot (Arbol_nj)
plot (Arbol_upgma)
plot (Arbol_nj, "unrooted")
dev.off()
alineamiento_1 <- msa (globinas, method = "ClustalW")
alineamiento_2 <- msa (globinas, method = "ClustalOmega")
Matriz_Distancias2 <- seqinr::dist.alignment (
msaConvert (alineamiento_2,"seqinr::alignment"))
Matriz_Distancias2
Arbol_nj2 <- ape::nj (Matriz_Distancias2)
plot (Arbol_nj2)
plot (Arbol_nj2, "unrooted")
Arbol_upgma2 <- phangorn::upgma (Matriz_Distancias2)
plot (Arbol_upgma2)
pdf ("Resultados/Arboles_globinas_ClustalOmega.pdf")
plot (Arbol_nj2)
plot (Arbol_upgma2)
plot (Arbol_nj2, "unrooted")
dev.off()
pdf ("Resultados/Arboles_globinas_ClustalW.pdf")
plot (Arbol_nj)
plot (Arbol_upgma)
plot (Arbol_nj, "unrooted")
dev.off()
Matriz_Distancias2 <- seqinr::dist.alignment (
msaConvert (alineamiento_2,"seqinr::alignment"))
Matriz_Distancias2
Arbol_nj2 <- ape::nj (Matriz_Distancias2)
plot (Arbol_nj2)
plot (Arbol_nj2, "unrooted")
pdf ("Resultados/Arboles_globinas_ClustalW.pdf")
plot (Arbol_nj)
plot (Arbol_nj, "unrooted")
dev.off()
pdf ("Resultados/Arboles_globinas_ClustalOmega.pdf")
plot (Arbol_nj2)
plot (Arbol_nj2, "unrooted")
dev.off()
